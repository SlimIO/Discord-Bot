{
    "name": "@slimio/discord-bot",
    "version": "0.1.0",
    "description": "Slimio discord bot",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "prepublishOnly": "pkg-ok",
        "test": "ava --verbose",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "nyc ava",
        "report": "nyc report --reporter=html"
    },
    "keywords": [
        "SlimIO",
        "monitoring"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "dependencies": {
        "body-parser": "^1.18.3",
        "discord.js": "^11.4.2",
        "dotenv": "^6.2.0",
        "make-promises-safe": "^5.0.0",
        "polka": "^0.5.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.0.0",
        "@commitlint/config-conventional": "^8.0.0",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^2.0.3",
        "@types/node": "^12.0.4",
        "ava": "^1.2.1",
        "cross-env": "^5.2.0",
        "eslint": "^5.13.0",
        "husky": "^1.3.1",
        "jsdoc": "^3.5.5",
        "nyc": "^13.2.0",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=10"
    },
    "husky": {
        "hooks": {
            "pre-push": "npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
